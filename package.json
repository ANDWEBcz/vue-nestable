{
  "name": "vue-nestable",
  "version": "2.4.0",
  "description": "vue-nestable",
  "author": "Ralph Huwiler <ralph@huwiler.rocks>",
  "homepage": "https://rhwilr.github.io/vue-nestable/",
  "license": "MIT",
  "module": "dist/vue-nestable.es.js",
  "main": "dist/vue-nestable.cjs.js",
  "unpkg": "dist/vue-nestable.iife.min.js",
  "jsdelivr": "dist/vue-nestable.iife.min.js",
  "keywords": [
    "vue",
    "vue-nestable",
    "vue nestable",
    "vue tree"
  ],
  "scripts": {
    "dev": "webpack-dev-server --mode development --open --hot",
    "build:lib": "rm -rf ./dist && bili src/index.js",
    "build:example": "rm -rf ./build && webpack --mode production --progress --hide-modules",
    "deploy-docs": "npm run build:example; gh-pages -d build",
    "test": "jest",
    "lint": "eslint --ext .js,.vue .",
    "fix": "eslint --ext .js,.vue . --fix",
    "prepublish": "npm run build:lib",
    "postpublish": "npm run deploy-docs",
    "pretest": "npm run lint"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie < 11"
  ],
  "dependencies": {
    "immutability-helper": "^3.0.0",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "bili": "^3.4.2",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "pre-commit": "^1.2.2",
    "rollup-plugin-vue": "^4.7.2",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "babel-jest"
    }
  },
  "pre-commit": [
    "test"
  ]
}
