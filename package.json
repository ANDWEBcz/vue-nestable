{
  "name": "vue-nestable",
  "version": "2.0.0",
  "description": "vue-nestable",
  "author": "Ralph Huwiler <ralph@huwiler.rocks>",
  "homepage": "https://rhwilr.github.io/vue-nestable/",
  "license": "MIT",
  "module": "dist/vue-nestable.es.js",
  "main": "dist/vue-nestable.cjs.min.js",
  "keywords": [
    "vue",
    "vue-nestable",
    "vue nestable",
    "vue tree"
  ],
  "scripts": {
    "dev": "webpack-dev-server --mode development --open --hot",
    "build:lib": "rm -rf ./dist && bili src/index.js",
    "build:example": "rm -rf ./build && webpack --mode production --progress --hide-modules",
    "deploy-docs": "npm run build:example; gh-pages -d build",
    "prepublish": "npm run build:lib",
    "postpublish": "npm run deploy-docs",
    "test": "jest"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie < 11"
  ],
  "dependencies": {
    "immutability-helper": "^3.0.0",
    "vue": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@vue/test-utils": "^1.0.0-beta.28",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.5",
    "bili": "^3.4.2",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.0.0",
    "pre-commit": "^1.2.2",
    "rollup-plugin-vue": "^4.6.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "vue-jest": "^3.0.2",
    "vue-loader": "^15.6.1",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "babel-jest"
    }
  },
  "pre-commit": [
    "test"
  ]
}
